cp ./CF6_16DEVAL10000/P100G100/cf6_16d_all_popmp100g100_seed01.out ./CF6_16DEVAL10000/P100G100/tmpp100g100.out
#Elimina del fichero todas las lineas excepto las de la ultima generación
sed -i "1,9900d" ./CF6_16DEVAL10000/P100G100/tmpp100g100.out
# Copia en un fichero los puntos que cumplen restricciones para calcular el  punto de referencia
cat ./CF6_16DEVAL10000/P100G100/tmpp100g100.out | awk '{if($3==0.0) print $1,"\t",$2;}'  > ./tmp_cf6_16d/p100g100.out
# Lee la primera columna del fichero de datos, obtiene el máximo y lo almacena en un fichero
cat ./tmp_cf6_16d/p100g100.out | awk '{print $1;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' > ./tmp_cf6_16d/hvref_1.out
# Lee la segunda columna del fichero de datos, obtiene el máximo y lo almacena en un fichero
cat ./tmp_cf6_16d/p100g100.out | awk '{print $2;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' > ./tmp_cf6_16d/hvref_2.out
# Lo mismo para el resto de ficheros de resultados
cp ./CF6_16DEVAL10000/P100G100/cf6_16d_all_popmp100g100_seed02.out ./CF6_16DEVAL10000/P100G100/tmpp100g100.out
sed -i "1,9900d" ./CF6_16DEVAL10000/P100G100/tmpp100g100.out
cat ./CF6_16DEVAL10000/P100G100/tmpp100g100.out | awk '{if($3==0.0) print $1,"\t",$2;}'  > ./tmp_cf6_16d/p100g100.out
cat ./tmp_cf6_16d/p100g100.out | awk '{print $1;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' >> ./tmp_cf6_16d/hvref_1.out
cat ./tmp_cf6_16d/p100g100.out | awk '{print $2;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' >> ./tmp_cf6_16d/hvref_2.out
