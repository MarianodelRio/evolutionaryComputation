# Lee la metrica de cada ejecucion y calcula la media. Almacena en un fichero
cat ./tmp_cf6_16d/hypervolp100g100_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_16d/hvp100g100mean.out
# Replica la media tantas veces como ejecuciones
cat ./tmp_cf6_16d/hvp100g100mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_16d/hvp100g100meanl.out
# Une en un fichero metrica y media
paste ./tmp_cf6_16d/hypervolp100g100_nsgaii_total.out ./tmp_cf6_16d/hvp100g100meanl.out | awk '{print $2,$3}' > ./tmp_cf6_16d/hvp100g100andmean.out 
# Resta media y metrica y eleva al cuadrado
cat ./tmp_cf6_16d/hvp100g100andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_16d/hvp100g100andmeansquare.out 
# Suma, divide por numero de ejecuciones menos 1 y saca raiz cuadrado para obtejer desviacion estandar
cat ./tmp_cf6_16d/hvp100g100andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_16d/hvp100g100std.out 
# Impime en pantalla media de la metrica
cat ./tmp_cf6_16d/hvp100g100mean.out | awk '{print "Configuration p100g100: Hypervolume mean: " $1}' 
# Impime en pantalla desviacion estandar de la metrica
cat ./tmp_cf6_16d/hvp100g100std.out | awk '{print "Configuration p100g100: Hypervolume standard deviation: " $1}' 
#Siguiente metrica
cat ./tmp_cf6_16d/spacingp100g100_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_16d/spp100g100mean.out
cat ./tmp_cf6_16d/spp100g100mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_16d/spp100g100meanl.out
paste ./tmp_cf6_16d/spacingp100g100_nsgaii_total.out ./tmp_cf6_16d/spp100g100meanl.out | awk '{print $2,$3}' > ./tmp_cf6_16d/spp100g100andmean.out 
cat ./tmp_cf6_16d/spp100g100andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_16d/spp100g100andmeansquare.out 
cat ./tmp_cf6_16d/spp100g100andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_16d/spp100g100std.out 
cat ./tmp_cf6_16d/spp100g100mean.out | awk '{print "Configuration p100g100: Spacing mean: " $1}' 
cat ./tmp_cf6_16d/spp100g100std.out | awk '{print "Configuration p100g100: Spacing standard deviation: " $1}' 

cat ./tmp_cf6_16d/hypervolp200g50_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_16d/hvp200g50mean.out
cat ./tmp_cf6_16d/hvp200g50mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_16d/hvp200g50meanl.out
paste ./tmp_cf6_16d/hypervolp200g50_nsgaii_total.out ./tmp_cf6_16d/hvp200g50meanl.out | awk '{print $2,$3}' > ./tmp_cf6_16d/hvp200g50andmean.out 
cat ./tmp_cf6_16d/hvp200g50andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_16d/hvp200g50andmeansquare.out 
cat ./tmp_cf6_16d/hvp200g50andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_16d/hvp200g50std.out 
cat ./tmp_cf6_16d/hvp200g50mean.out | awk '{print "Configuration p200g50: Hypervolume mean: " $1}' 
cat ./tmp_cf6_16d/hvp200g50std.out | awk '{print "Configuration p200g50: Hypervolume standard deviation: " $1}' 
cat ./tmp_cf6_16d/spacingp200g50_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_16d/spp200g50mean.out
cat ./tmp_cf6_16d/spp200g50mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_16d/spp200g50meanl.out
paste ./tmp_cf6_16d/spacingp200g50_nsgaii_total.out ./tmp_cf6_16d/spp200g50meanl.out | awk '{print $2,$3}' > ./tmp_cf6_16d/spp200g50andmean.out 
