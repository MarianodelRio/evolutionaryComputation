cat ./tmp_cf6_16d/spp200g50andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_16d/spp200g50std.out 
cat ./tmp_cf6_16d/spp200g50mean.out | awk '{print "Configuration p200g50: Spacing mean: " $1}' 
cat ./tmp_cf6_16d/spp200g50std.out | awk '{print "Configuration p200g50: Spacing standard deviation: " $1}' 

