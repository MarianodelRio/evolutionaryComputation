sed -i "1,9960d" ./CF6_16DEVAL10000/P40G250/tmpp40g250.out
cat ./CF6_16DEVAL10000/P40G250/tmpp40g250.out | awk '{if($3==0.0) print $1,"\t",$2;}'  > ./tmp_cf6_16d/p40g250.out
cat ./tmp_cf6_16d/p40g250.out | awk '{print $1;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' >> ./tmp_cf6_16d/hvref_1.out
