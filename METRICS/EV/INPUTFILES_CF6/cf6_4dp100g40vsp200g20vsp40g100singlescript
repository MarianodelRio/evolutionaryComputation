sed -i "1,3960d" ./CF6_4DEVAL4000/P40G100/tmpp40g100.out
cat ./CF6_4DEVAL4000/P40G100/tmpp40g100.out | awk '{if($3==0.0) print $1,"\t",$2;}'  > ./tmp_cf6_4d/p40g100.out
cat ./tmp_cf6_4d/p40g100.out | awk '{print $1;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' >> ./tmp_cf6_4d/hvref_1.out
cat ./tmp_cf6_4d/p40g100.out | awk '{print $2;}' | awk '{if(max==""){max=$1}; if($1>max) {max=$1}} END {print max}' >> ./tmp_cf6_4d/hvref_2.out
