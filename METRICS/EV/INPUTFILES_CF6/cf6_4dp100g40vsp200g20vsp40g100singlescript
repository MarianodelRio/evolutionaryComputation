# Lee la metrica de cada ejecucion y calcula la media. Almacena en un fichero
cat ./tmp_cf6_4d/hypervolp100g40_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_4d/hvp100g40mean.out
# Replica la media tantas veces como ejecuciones
cat ./tmp_cf6_4d/hvp100g40mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_4d/hvp100g40meanl.out
# Une en un fichero metrica y media
paste ./tmp_cf6_4d/hypervolp100g40_nsgaii_total.out ./tmp_cf6_4d/hvp100g40meanl.out | awk '{print $2,$3}' > ./tmp_cf6_4d/hvp100g40andmean.out 
# Resta media y metrica y eleva al cuadrado
cat ./tmp_cf6_4d/hvp100g40andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_4d/hvp100g40andmeansquare.out 
# Suma, divide por numero de ejecuciones menos 1 y saca raiz cuadrado para obtejer desviacion estandar
cat ./tmp_cf6_4d/hvp100g40andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_4d/hvp100g40std.out 
# Impime en pantalla media de la metrica
cat ./tmp_cf6_4d/hvp100g40mean.out | awk '{print "Configuration p100g40: Hypervolume mean: " $1}' 
# Impime en pantalla desviacion estandar de la metrica
cat ./tmp_cf6_4d/hvp100g40std.out | awk '{print "Configuration p100g40: Hypervolume standard deviation: " $1}' 
#Siguiente metrica
cat ./tmp_cf6_4d/spacingp100g40_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_4d/spp100g40mean.out
cat ./tmp_cf6_4d/spp100g40mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_4d/spp100g40meanl.out
paste ./tmp_cf6_4d/spacingp100g40_nsgaii_total.out ./tmp_cf6_4d/spp100g40meanl.out | awk '{print $2,$3}' > ./tmp_cf6_4d/spp100g40andmean.out 
cat ./tmp_cf6_4d/spp100g40andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_4d/spp100g40andmeansquare.out 
cat ./tmp_cf6_4d/spp100g40andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_4d/spp100g40std.out 
cat ./tmp_cf6_4d/spp100g40mean.out | awk '{print "Configuration p100g40: Spacing mean: " $1}' 
cat ./tmp_cf6_4d/spp100g40std.out | awk '{print "Configuration p100g40: Spacing standard deviation: " $1}' 

cat ./tmp_cf6_4d/hypervolp200g20_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_4d/hvp200g20mean.out
cat ./tmp_cf6_4d/hvp200g20mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_4d/hvp200g20meanl.out
paste ./tmp_cf6_4d/hypervolp200g20_nsgaii_total.out ./tmp_cf6_4d/hvp200g20meanl.out | awk '{print $2,$3}' > ./tmp_cf6_4d/hvp200g20andmean.out 
cat ./tmp_cf6_4d/hvp200g20andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_4d/hvp200g20andmeansquare.out 
cat ./tmp_cf6_4d/hvp200g20andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_4d/hvp200g20std.out 
cat ./tmp_cf6_4d/hvp200g20mean.out | awk '{print "Configuration p200g20: Hypervolume mean: " $1}' 
cat ./tmp_cf6_4d/hvp200g20std.out | awk '{print "Configuration p200g20: Hypervolume standard deviation: " $1}' 
cat ./tmp_cf6_4d/spacingp200g20_nsgaii_total.out | awk '{print $2;}' | awk '{total+=$1; count+=1} END {print total/count, count}' > ./tmp_cf6_4d/spp200g20mean.out
cat ./tmp_cf6_4d/spp200g20mean.out | awk '{for (i=1; i<=$2; i++) print $1}' >./tmp_cf6_4d/spp200g20meanl.out
paste ./tmp_cf6_4d/spacingp200g20_nsgaii_total.out ./tmp_cf6_4d/spp200g20meanl.out | awk '{print $2,$3}' > ./tmp_cf6_4d/spp200g20andmean.out 
cat ./tmp_cf6_4d/spp200g20andmean.out | awk '{print $1, "\t", $2, "\t", ($1-$2)*($1-$2)}' > ./tmp_cf6_4d/spp200g20andmeansquare.out 
cat ./tmp_cf6_4d/spp200g20andmeansquare.out | awk '{print $3;}' | awk '{total+=$1; count+=1} END {print sqrt(total/(count-1))}' > ./tmp_cf6_4d/spp200g20std.out 
cat ./tmp_cf6_4d/spp200g20mean.out | awk '{print "Configuration p200g20: Spacing mean: " $1}' 
cat ./tmp_cf6_4d/spp200g20std.out | awk '{print "Configuration p200g20: Spacing standard deviation: " $1}' 

